{
  "name": "@usagi-coffee/tree-sitter-abl",
  "version": "0.0.46",
  "description": "ABL grammar for tree-sitter",
  "main": "bindings/node",
  "scripts": {
    "install": "node-gyp-build || echo 'Using prebuilt binary'",
    "generate": "tree-sitter generate",
    "build:wasm": "tree-sitter build-wasm",
    "parse": "tree-sitter parse",
    "test": "tree-sitter test",
    "dev": "nodemon --ignore ./src -e js,json,p --exec 'tree-sitter parse example.p'",
    "prepublishOnly": "npm run build:wasm"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@usagi-coffee/tree-sitter-abl": "file:usagi-coffee-tree-sitter-abl-0.0.46.tgz",
    "nan": "^2.22.2",
    "node-addon-api": "^8.3.1",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.3"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BalticAmadeus/AblFormatter.git"
  },
  "bugs": {
    "url": "https://github.com/BalticAmadeus/AblFormatter/issues"
